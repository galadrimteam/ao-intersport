{
  "expo": {
    "name": "intersport",
    "slug": "intersport",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/intersport-2.png",
    "scheme": "intersport",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/intersport-2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/intersport-2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": " Afin de scanner le code bar, $(PRODUCT_NAME) a besoin de votre permission pour accéder à votre caméra.",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
