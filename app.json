{
  "expo": {
    "name": "intersport",
    "slug": "intersport",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/intersport-2.png",
    "scheme": "intersport",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/intersport-2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/intersport-2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": " Afin de scanner le code bar, $(PRODUCT_NAME) a besoin de votre permission pour accéder à votre caméra.",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f13a4e41-74b5-4bdc-9f5b-5703e5d63525"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f13a4e41-74b5-4bdc-9f5b-5703e5d63525"
    },
    "owner": "intersport"
  }
}
